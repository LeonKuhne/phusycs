#!/bin/bash

function run() {
  cargo build --features bevy/dynamic
  RUST_LOG=phusycs=debug cargo run --features bevy/dynamic
}

function test() {
  cargo build --features bevy/dynamic
  RUST_BACKTRACE=1 RUST_LOG=phusycs=debug cargo run --features bevy/dynamic
}

function wipe() {
  rm -rf Cargo.lock target
}

title "Commands:"
help .cd | indent "$C_DEBUG>"
cat .todo
